
                

          <p id="notice"><%= notice %></p>

          <h1>Listado de administradores</h1>

          <table>
            <thead>
              <tr>
                <th>Nombre</th>
                <th>Email</th>
                <th class="color-blanco-botones-tabla" colspan="2"></th>
              </tr>
            </thead>

            <tbody>
              <% @administrador.each do |administrador| %>
                <tr>
                  <td class="padding_table"><%= administrador.nombre %></td>
                  <td class="padding_table"><%= administrador.email %></td>

                  <td class="color-blanco-botones-tabla">


                     <%= button_to "",administrador_path(administrador) , :method => :get , class: "imagen_ver" %>

                    <%= link_to 'Show', administrador_path(administrador) %>

                  
                  </td>
                  <td class = "color-blanco-botones-tabla ">
 
                   
                     <%= button_to "" , { :action => :destroy, :id => administrador.id }, :method => :delete, :data => { :confirm => 'Estas seguro de elimiarlo de los registros?' } , class: "imagen_eliminar" %>


  


                  </td>

                </tr>
              <% end %>

  




            </tbody>
          </table>
