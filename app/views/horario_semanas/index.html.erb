<p id="notice"><%= notice %></p>

<h1>Horario Semanas</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Fecha inicio</th>
      <th>Fecha termino</th>
      <th>Lunes</th>
      <th>Martes</th>
      <th>Miercoles</th>
      <th>Jueves</th>
      <th>Viernes</th>
      <th>Sabado</th>
      <th>Domingo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @horario_semanas.each do |horario_semana| %>
      <tr>
        <td><%= horario_semana.nombre %></td>
        <td><%= horario_semana.fecha_inicio %></td>
        <td><%= horario_semana.fecha_termino %></td>
        <td><%= horario_semana.lunes_id %></td>
        <td><%= horario_semana.martes_id %></td>
        <td><%= horario_semana.miercoles_id %></td>
        <td><%= horario_semana.jueves_id %></td>
        <td><%= horario_semana.viernes_id %></td>
        <td><%= horario_semana.sabado_id %></td>
        <td><%= horario_semana.domingo_id %></td>
        <td><%= link_to 'Show', horario_semana %></td>
        <td><%= link_to 'Edit', edit_horario_semana_path(horario_semana) %></td>
        <td><%= link_to 'Destroy', horario_semana, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Horario Semana', new_horario_semana_path %>
