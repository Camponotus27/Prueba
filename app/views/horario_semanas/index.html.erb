<p id="notice"><%= notice %></p>

<h1>Horario Semanas</h1>

<table>
  <thead>
    <tr>
      <th>Fecha ini</th>
      <th>Fecha ter</th>
      <th>Dia horario semana lunes</th>
      <th>Dia horario semana martes</th>
      <th>Dia horario semana miercoles</th>
      <th>Dia horario semana jueves</th>
      <th>Dia horario semana viernes</th>
      <th>Dia horario semana sabado</th>
      <th>Dia horario semana domingo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @horario_semanas.each do |horario_semana| %>
      <tr>
        <td><%= horario_semana.fecha_ini %></td>
        <td><%= horario_semana.fecha_ter %></td>
        <td><%= horario_semana.dia_horario_semana_lunes %></td>
        <td><%= horario_semana.dia_horario_semana_martes %></td>
        <td><%= horario_semana.dia_horario_semana_miercoles %></td>
        <td><%= horario_semana.dia_horario_semana_jueves %></td>
        <td><%= horario_semana.dia_horario_semana_viernes %></td>
        <td><%= horario_semana.dia_horario_semana_sabado %></td>
        <td><%= horario_semana.dia_horario_semana_domingo %></td>
        <td><%= link_to 'Show', horario_semana %></td>
        <td><%= link_to 'Edit', edit_horario_semana_path(horario_semana) %></td>
        <td><%= link_to 'Destroy', horario_semana, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Horario Semana', new_horario_semana_path %>
