<p id="notice"><%= notice %></p>

<h1>Usercomunas</h1>

<table>
  <thead>
    <tr>
      <th>Comuna</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usercomunas.each do |usercomuna| %>
      <tr>
        <td><%= usercomuna.comuna %></td>
        <td><%= usercomuna.user %></td>
        <td><%= link_to 'Show', usercomuna %></td>
        <td><%= link_to 'Edit', edit_usercomuna_path(usercomuna) %></td>
        <td><%= link_to 'Destroy', usercomuna, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usercomuna', new_usercomuna_path %>
