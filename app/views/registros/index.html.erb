<p id="notice"><%= notice %></p>

<h1>Registros</h1>

<table>
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Fecha</th>
      <th>Profesional</th>
      <th>Cliente</th>
      <th>Comuna atencion</th>
      <th>Servicio</th>
      <th>Horario dia</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registros.each do |registro| %>
      <tr>
        <td><%= registro.codigo %></td>
        <td><%= registro.fecha %></td>
        <td><%= registro.Profesional %></td>
        <td><%= registro.cliente %></td>
        <td><%= registro.comuna_atencion %></td>
        <td><%= registro.servicio %></td>
        <td><%= registro.horario_dia %></td>
        <td><%= link_to 'Show', registro %></td>
        <td><%= link_to 'Edit', edit_registro_path(registro) %></td>
        <td><%= link_to 'Destroy', registro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registro', new_registro_path %>
